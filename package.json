  "scripts": {
    "start": "clear; npm run dev-server",
    "dev-server": "echo 'starting development server...' && NODE_ENV=development babel-node server",
    "prod-server": "echo 'starting production server...' && NODE_ENV=production PORT=$PORT babel-node server",
    "deploy": "git push heroku",
    "encrypt": "nodecipher encrypt -i api/api-config.js -o api/api-config.js.cast5",
    "decrypt": "nodecipher decrypt -i api/api-config.js.cast5 -o api/api-config.js",
    "test": "NODE_ENV=test npm run test:once -- --watch",
    "test:once": "mocha --compilers js:babel-core/register --recursive ",
    "test:debug": "node-debug _mocha --compilers js:babel-core/register --recursive & wait",
    "build": "echo 'building dist...' && rimraf dist && NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors"
  },
  "main": "server.js",
  "keywords": []
}
